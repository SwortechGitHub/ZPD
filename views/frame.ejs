<!DOCTYPE html>
<html lang="lv">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title%></title>
    <link rel="stylesheet" href="/css/frame.css">
    <script src="/js/frame.js"></script>
</head>
<body>
  <% if(css.length > 0){ %>
    <%- "<style>"+css+"</style>"%>
    <%}%>
    <nav id='menu'>
        <input type='checkbox' id='responsive-menu' onclick='updatemenu()'><label></label>
        <ul>
          <% webPages.forEach(page => { %>
            <% if (page.parent === "none") { %>
              <li>
                <a href="<%= page.route %>"><%= page.name %></a>
              </li>
            <% }%>
          <% }); %>
          <li>
            <a class='dropdown-arrow'>MacÄ«bas</a>
            <ul class='sub-menus'>
              <% webPages.filter(childPage => childPage.parent === "Macibas").forEach(childPage => { %>
                <li><a href="<%= childPage.route %>"><%= childPage.name %></a></li>
              <% }); %>
            </ul>
          </li>
          <li>
            <a class='dropdown-arrow'>Skola</a>
            <ul class='sub-menus'>
              <% webPages.filter(childPage => childPage.parent === "Skola").forEach(childPage => { %>
                <li><a href="<%= childPage.route %>"><%= childPage.name %></a></li>
              <% }); %>
            </ul>
          </li>
        </ul>
      </nav>
    <header></header>
    <% if(html.length > 0){ %>
      <%- html %>
    <% } %>
    <footer></footer>
</body>
</html>